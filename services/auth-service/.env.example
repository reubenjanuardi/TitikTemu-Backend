# Database URL - Supabase PostgreSQL with auth schema
# Format: postgresql://username:password@host:port/database?schema=schema_name
DATABASE_URL="postgresql://auth_service_user:your_password@your_supabase_host:5432/postgres?schema=auth_schema"

# JWT Configuration
JWT_SECRET="your_jwt_secret_key_here"
JWT_EXPIRE="7d"

# Environment
NODE_ENV="development"
PORT=3001
